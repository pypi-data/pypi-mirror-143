{
    "scope":{
      "source":"serotracker",
      "source_description":"National seroprevalence studies reported by SeroTracekr. SeroTracker is a dashboard and data platform for SARS-CoV-2 serosurveys. Their team conduct an ongoing systematic review to track serosurveys (antibody testing-based surveillance efforts) around the world and visualize findings on this dashboard. Antibodies against SARS-CoV-2, the virus that causes COVID-19, indicate previous infection or vaccination.",
      "tags":["SeroTracker"],
      "data_quality":"Survey (third-party compilation)",
      "frequency":"daily",
      "frequency_start_hour":5,
      "frequency_end_hour":5,
      "reference_user":"Rahul Arora (University of Oxford) and Tingting Yan (University of Toronto)",
      "reporting_email":"",
      "globals":[
        {"variable":"source"},
        {"variable":"pathogen_name", "value":"COVID-19"}
      ],
      "update_scope":[
        {"variable":"source"},
        {"variable":"reporting_date"}
      ]
    },
    "acquisition":{
      "channel":{
        "name":"git",
        "branch":"main",
        "url": "https://github.com/serotracker/sars-cov-2-data.git",
        "match": "serotracker_dataset\\.csv"
      },
      "format": {
        "name":"csv",
        "decimal_sign":".",
        "thousands_separator":"",
        "date_format":"%Y-%m-%d",
        "encoding":"UTF-8"
      }
    },
    "columns":[
      {"name":"estimate_name", "variable":"survey_estimate_name"},
      {"name":"study_name", "variable":"survey_name"},
      {"name":"source_name", "variable":"survey_source_name"},
      {"name":"publication_date", "variable":"survey_publication_date"},
      {"name":"source_type", "variable":"sourvey_source_type"},
      {"name":"estimate_grade", "variable":"survey_estimate_grade"},
      {"name":"study_type", "variable":"survey_study_type"},
      {"name":"country"},
      {"name":"state"},
      {"name":"city"},
      {"name":"study_inclusion_criteria"},
      {"name":"study_exclusion_criteria"},
      {"name":"sampling_start_date"},
      {"name":"sampling_end_date", "variable":"reporting_date"},
      {"name":"population_group", "variable":"population_group"},
      {"name":"sex", "variable":"gender_name"},
      {"name":"age"},
      {"name":"age_min"},
      {"name":"age_max"},
      {"name":"age_group", "variable":"age_group"},
      {"name":"subgroup_var", "variable":""},
      {"name":"subgroup_cat", "variable":""},
      {"name":"subgroup_specific_category", "variable":""},
      {"name":"denominator_value", "variable":"population"},
      {"name":"serum_pos_prevalence", "variable":"sero_prevalence"},
      {"name":"seroprev_95_ci_lower", "variable":""},
      {"name":"seroprev_95_ci_upper", "variable":""},
      {"name":"dashboard_primary_estimate", "variable":""},
      {"name":"test_adj", "variable":""},
      {"name":"pop_adj", "variable":""},
      {"name":"clustering_adjustment", "variable":""},
      {"name":"sampling_method", "variable":""},
      {"name":"test_name", "variable":""},
      {"name":"test_manufacturer", "variable":""},
      {"name":"test_type", "variable":""},
      {"name":"specimen_type", "variable":""},
      {"name":"isotypes", "variable":""},
      {"name":"antibody_target", "variable":""},
      {"name":"test_validation", "variable":""},
      {"name":"sensitivity", "variable":""},
      {"name":"specificity", "variable":""},
      {"name":"overall_risk_of_bias", "variable":""},
      {"name":"jbi_1", "variable":""},
      {"name":"jbi_2", "variable":""},
      {"name":"jbi_3", "variable":""},
      {"name":"jbi_4", "variable":""},
      {"name":"jbi_5", "variable":""},
      {"name":"jbi_6", "variable":""},
      {"name":"jbi_7", "variable":""},
      {"name":"jbi_8", "variable":""},
      {"name":"jbi_9", "variable":""},
      {"name":"first_author", "variable":"survey_first_author"},
      {"name":"lead_institution", "variable":"survey_institution"},
      {"name":"is_unity_aligned", "variable":""},
      {"name":"url", "variable":"survey_url"},
      {"name":"date_created", "variable":""},
      {"name":"last_modified_time", "variable":""},
      {"name":"data_quality_status", "variable":"survey_data_quality"},
      {"name":"alpha_3_code", "variable":"iso_country_code_3"}
    ]
}
