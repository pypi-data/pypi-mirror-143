[Unit]
Description="{{ neocli_name }}-redis-queue"
PartOf={{ neocli_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_queue_config }}
StandardOutput=file:{{ neocli_dir }}/logs/redis-queue.log
StandardError=file:{{ neocli_dir }}/logs/redis-queue.error.log
WorkingDirectory={{ sites_dir }}
