[Unit]
Description="{{ neocli_name }}-redis-cache"
PartOf={{ neocli_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_cache_config }}
StandardOutput=file:{{ neocli_dir }}/logs/redis-cache.log
StandardError=file:{{ neocli_dir }}/logs/redis-cache.error.log
WorkingDirectory={{ sites_dir }}
